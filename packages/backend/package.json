  "description": "\"# keagate\"",
  "main": "build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "shx rm tsconfig.package.tsbuildinfo",
    "build": "tsc -p ./tsconfig.package.json",
